{
  "name": "order-management-backend",
  "version": "1.0.0",
  "description": "Backend API for Order Management System",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "mcp": "node dist/mcp/mcp-server.js",
